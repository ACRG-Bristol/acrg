                timeframe=timeframe
            endif else begin
              topog=0
            endelse
            
            ;-----------------------------------------------------------------------
            ; loop through fields and species
            
            for ifield=startfield,endfield-1 do begin
            
              if (namever eq 3)then begin
                ; NAME III
                field=fieldlist(ifield)
                level=levellist(ifield)
