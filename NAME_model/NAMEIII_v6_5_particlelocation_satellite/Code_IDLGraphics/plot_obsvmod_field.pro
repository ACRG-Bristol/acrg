              if (namever eq 3)then begin
                ; NAME III
                TimeAveraging=TimeAveragingList(ifield)
                FieldName=FieldNameList(ifield)
                datavalue=datavaluelist(ifield)
                
                selgrid1 = STRING(selgrid,'_C',casenum,'_T',it+1)
                selgrid1 = STRCOMPRESS(selgrid1, /Remove_All)
                
                if (sumpm eq 1) and (species eq 'TOTAL-PM10') then begin
                  readfield,datadir,filetime,selgrid1,field,level,$
                    'PM10',lon1,lat1,dat_a,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe,$
                    timeaveraging=timeaveraging,seldatavalue=datavalue,selname=FieldName
                  readfield,datadir,filetime,selgrid1,field,level,$
                    'SULPHATE',lon1,lat1,dat_b,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe,$
                    timeaveraging=timeaveraging,seldatavalue=datavalue,selname=FieldName
                  readfield,datadir,filetime,selgrid1,field,level,$
                    'NH42SO4',lon1,lat1,dat_c,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe,$
                    timeaveraging=timeaveraging,seldatavalue=datavalue,selname=FieldName
                  readfield,datadir,filetime,selgrid1,field,level,$
                    'NH4NO3',lon1,lat1,dat_d,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe,$
                    timeaveraging=timeaveraging,seldatavalue=datavalue,selname=FieldName
                  readfield,datadir,filetime,selgrid1,field,level,$
                    'NAER',lon1,lat1,dat_e,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe,$
                    timeaveraging=timeaveraging,seldatavalue=datavalue,selname=FieldName
                  dat1=dat_a+dat_b+dat_c+dat_d+dat_e
                  head1[3]='TOTAL-PM10'
                endif else begin
                  readfield,datadir,filetime,selgrid1,field,level,$
                    species,lon1,lat1,dat1,head1,namever=namever,timeframe=timeframe,$
                    timeaveraging=timeaveraging,seldatavalue=datavalue,selname=FieldName
                endelse  
                  
              endif else begin
                ; NAME
                
                if (sumpm eq 1) and (species eq 'TOTAL-PM10') then begin  
                  readfield,datadir,filetime,selgrid,field,level,$
                    'PM10',lon1,lat1,dat_a,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe
                  readfield,datadir,filetime,selgrid,field,level,$
                    'SULPHATE',lon1,lat1,dat_b,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe
                  readfield,datadir,filetime,selgrid,field,level,$
                    'NH42SO4',lon1,lat1,dat_c,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe
                  readfield,datadir,filetime,selgrid,field,level,$
                    'NH4NO3',lon1,lat1,dat_d,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe
                  readfield,datadir,filetime,selgrid,field,level,$
                    'NAER',lon1,lat1,dat_e,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe
                  dat1=dat_a+dat_b+dat_c+dat_d+dat_e
                  head1[1]='TOTAL-PM10'
                endif else begin
                  readfield,datadir,filetime,selgrid,field,level,$
                    species,lon1,lat1,dat1,head1,namever=namever,timeframe=timeframe
                endelse
                  
              endelse
              
              dat1=dat1*scalefactor
              
              pos=(ispec) mod (nxplot*nyplot)
              multiplot=[pos,nxplot,nyplot]
              
              if(back ne 0 and strpos(field,'Dosage') ge 0)then begin
                if(percent eq 0)then begin
                  head1(3)='Back Run Time Integrated Air concentration'
                  head1(4)=''
                endif else begin
                  dat1=(dat1/total(dat1))*100.0
                  head1(3)='Percentage contribution'
                  head1(4)='%'
                endelse
                lenlevel=STRLEN(level)
                attribstr1=attribstr+STRMID(level,4,lenlevel-4)
              endif
              
              
              if(back ne 0 and strpos(field,'Air concentration') ge 0)then begin
                if(percent eq 0)then begin
                  head1(3)='Back Run Air concentration'
                  head1(4)=''
                endif else begin
                  if(strpos(modtitle(6),'multiple') lt 0)then begin
                    rate=FLOAT(strmid(modtitle(6),20,9))
                    totalmass=rate*relduration
                    
                    dat1=dat1/totalmass*100.0
                    
                    head1(3)='Percentage contribution per m^3'
                    head1(4)='%/m^3'
                    
                  endif else begin
                    print,'error in percent calculation - illegal options'
                    stop
                  endelse
                endelse
              endif
              
              
              
              ;-----------------------------------------------------------------------
              ; plot fields
              
              contours1=reform(contours(ifield,ispec,*))
              
              if (namever eq 3)then begin
                ; NAME III
                if(back ne 0)then begin
                  postitle=attribstr1
                endif else begin
                  postitle=species
                endelse
              endif else begin
                ; NAME
                if(back ne 0)then begin
                  postitle=attribstr1
                endif else begin
                  postitle=species
                endelse
              endelse
              
              plotfieldpos,lon1,lat1,dat1,position=position,type=type,$
                zoom=zoom,contourlevels=contours1,contourcolors=contourcolors,$
                xyrel=xyrel,fieldunits=head1(4),multiplot=multiplot,$
                modtitle=postitle,plotloc=plotloc,$
                plotlegend=plotlegend,pmsl=pmsl,u=u,v=v,topog=topog,$
                highres=highres,projection=projection,tlon=tlon,tlat=tlat,$
                plotlincomtopog=plotlincomtopog,polar=polar,$
                countries=countries,coasts=coasts,states=states,$
                placenames=placenames,origin=origin,automap=automap,$
                plotvaacobs=plotvaacobs,tekcolors=tekcolors
                
              ;-----------------------------------------------------------------------     
              ; plot AQ obs

              if (plotAQobsconc eq 1) then begin
                                 
               ;Read obs sites info
               readdata,AQobslist,obssites,nobssites
               
               for i_obssite=0,nobssites-1 do begin
               
                 ;check obs type to be plotted
                 if keyword_set(AQobstypes) then begin
                   indexobs=where(AQobstypes eq strtrim(obssites(4,i_obssite),2),countobs)
                   if countobs le 0 then continue
                 endif   
               
                 ;check within max limits
                 if ( obssites(1,i_obssite) le ymax ) and $
                    ( obssites(1,i_obssite) ge ymin ) and $         
                    ( obssites(2,i_obssite) le xmax ) and $
                    ( obssites(2,i_obssite) ge xmin ) then begin
                    
                   ;read obs data
                   ;convert filetime to date array
                   date={datetime,year:0,month:0,day:0,hour:0,minute:0,second:0} 
                   date.year=strmid(filetime,0,4)
                   date.month=strmid(filetime,4,2)
                   date.day=strmid(filetime,6,2)
                   date.hour=strmid(filetime,8,2) 
                    
                   ;read obsdata
                   readaqobs,AQobsdir,strtrim(obssites(3,i_obssite),2),date,$
                             obsdata,namespecies=species,tmpdir=datadir[0] 
                   print,'Obs data:',obsdata            
                   ;obsdata - float containing value at that time and location     
                        
                   ;Determine colour
                   AQcolour=axiscol
                   for i_contour=0,n_elements(contours1)-2 do begin
                     AQcolourindex=Where( (obsdata ge contours1(i_contour) ) $
                                   and (obsdata lt contours1(i_contour+1)),$
                                   count )
                     if count gt 0 then begin
                       AQcolour=contourcolors(i_contour)
                     endif
                   endfor   
                   if obsdata ne -999.9 then begin
                     if (AQcolour eq axiscol) then begin
                       print,'Value outside of contour range: ',obsdata,'( ',obssites(3,i_obssite),' ',species,' )'
                     endif else begin
      
                       ;Plot concentration within a filled, outlined circle              
                       A = findgen(17) * (!PI*2/16.)
                       usersym, cos(A), sin(A), /fill
                       plots,obssites(2,i_obssite),obssites(1,i_obssite),psym=8,$
                             color=AQcolour,symsize=1
                       usersym, cos(A), sin(A)
                       plots,obssites(2,i_obssite),obssites(1,i_obssite),psym=8,$
                             color=axiscol,symsize=1
                         
                       ;Label obs sites if necessary
                       if plotAQlocnames eq 1 then begin
                         xyouts,obssites(2,i_obssite),obssites(1,i_obssite),$
                                strtrim(obssites(3,i_obssite),2),color=axiscol,$
                                charsize=0.5
                       endif 
                     
                     endelse  ;ne axiscolor
                    endif  ;ne -999.9
                   
                 endif ;maplimits
               endfor ;i_obssite         
              endif ;plotAQobs  

              ;-----------------------------------------------------------------------
              ; annotation
                
                
              if(plotheader ne 0 and pos eq 0)then begin
              
                xyouts,0.5,0.98,strtrim(modtitle(0),2),$
                  /normal,alignment=0.5,charsize=1.3,color=axiscol
                  
                xyouts,0.5,0.94,strcompress(modtitle(1)),$
                  /normal,alignment=0.5,charsize=1.3,color=axiscol
                  
                if (namever eq 3)then begin
                  ; NAME III
                  if (back ne 0) then begin
                    xyouts,0.5,0.90,'Attribution back to '+head1(13),$
                      /normal,charsize=xysize*1.2,alignment=0.5,color=axiscol
                  endif else begin
                    if printtimeaverage eq 1 then begin
                      timeavstring,head1(13),head1(7),validitytime
                      xyouts,0.5,0.90,validitytime,$
                        /normal,charsize=xysize*1.2,alignment=0.5,color=axiscol
                    endif else begin
                      xyouts,0.5,0.90,'Valid at '+head1(13),$
                        /normal,charsize=xysize*1.2,alignment=0.5,color=axiscol
                    endelse
                  endelse
                endif else begin
                  ; NAME
                  if (back ne 0) then begin
                    xyouts,0.5,0.90,'Attribution back to '+head1(6),$
                      /normal,charsize=xysize*1.2,alignment=0.5,color=axiscol
                  endif else begin
                    if printtimeaverage eq 1 then begin
                      timeavstring,head1(6),head1(2),validitytime
                      xyouts,0.5,0.90,validitytime,$
                        /normal,charsize=xysize*1.2,alignment=0.5,color=axiscol
                    endif else begin
                      xyouts,0.5,0.90,'Valid at '+head1(6),$
                        /normal,charsize=xysize*1.2,alignment=0.5,color=axiscol
                    endelse
                  endelse
                endelse
                
                xyouts,0.05,0.12,modtitle(4),/normal,charsize=0.9,color=axiscol
                xyouts,0.05,0.10,modtitle(5),/normal,charsize=0.9,color=axiscol
                xyouts,0.05,0.08,modtitle(6),/normal,charsize=0.9,color=axiscol
                xyouts,0.05,0.06,modtitle(7),/normal,charsize=0.9,color=axiscol
                xyouts,0.05,0.04,modtitle(8),/normal,charsize=0.9,color=axiscol
                if (namever eq 3)then begin
                  ; NAME III
                  xyouts,0.60,0.12,field+' '+level,$
                    /normal,charsize=0.9,color=axiscol
                  ;
                  if (datavaluelist(ifield) ne ' ')then begin
                    ; NAME III data value
                    tokens=strsplit(datavaluelist(ifield), '=', /extract)
                    dvalue=strtrim(tokens(1), 2)
                    xyouts,0.60,0.02,'Data value: '+dvalue,$
                      /normal,charsize=0.9,color=axiscol
                  endif
                endif else begin
                  ; NAME
                  xyouts,0.60,0.12,field+' '+level,$
                    /normal,charsize=0.9,color=axiscol
                endelse
                xyouts,0.6,0.10,modtitle(3),/normal,charsize=0.9,color=axiscol
                xyouts,0.6,0.08,modtitle(2),/normal,charsize=0.9,color=axiscol
                if (printcasenum eq 1)then begin
                  ; NAME III case number
                  xyouts,0.60,0.04,'Case number: '+casenum,$
                    /normal,charsize=0.9,color=axiscol
                endif
                
                if (metoffice eq 1) then begin
                  xyouts,0.5,0.00,'Met Office Crown copyright',$
                    /normal,alignment=0.5,charsize=0.8,color=axiscol
                    
                  image=read_image('MO_Master_B.jpg')
                  loadct,0
                  tv,image,0.82,0.91,true=1,xsize=0.15,/normal
                  if (ct_blue2red eq 0) then begin
                    loadct,ct
                    tek_color,0,tekcolors
                  endif else begin
                    TVLCT,red,green,blue
                  endelse
                endif else if (metoffice eq 2) then begin
                  xyouts,0.5,0.00,'Met Office Crown copyright',$
                    /normal,alignment=0.5,charsize=0.8,color=axiscol
                    
                  image=read_image('MO_Master_Mono_B.jpg')
                  loadct,0
                  tv,image,0.82,0.91,true=1,xsize=0.15,/normal
                  if (ct_blue2red eq 0) then begin
                    loadct,ct
                    tek_color,0,tekcolors
                  endif else begin
                    TVLCT,red,green,blue
                  endelse
                endif
                
              endif
              
              
              ;-----------------------------------------------------------------------
              ; new page
              
              if(pos eq ((nxplot*nyplot)-1) or ifield eq endspecies-1)then begin
                erase
              endif
              
            endfor
          endfor
          
          device,/close_file
          
          
          ;-----------------------------------------------------------------------
          ; generate gifs
          
          if((gengif eq 1 or genjpg eq 1 or genanim eq 1) and pc eq 0)then begin
            genanim,file,datadir,filetem,gengif=gengif,genjpg=genjpg,$
              genanim=genanim,method=method,xpixel=xpixel
          endif
          
        endfor
      endfor
      
    end
    
  endcase
  
end
