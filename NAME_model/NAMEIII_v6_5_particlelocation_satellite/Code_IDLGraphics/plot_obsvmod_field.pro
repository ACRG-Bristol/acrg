                    namever=namever,timeframe=timeframe,$
                    timeaveraging=timeaveraging,seldatavalue=datavalue,selname=FieldName
                  readfield,datadir,filetime,selgrid1,field,level,$
                    'NH42SO4',lon1,lat1,dat_c,head1,modtitle=modtitle,$
                    namever=namever,timeframe=timeframe,$
                    timeaveraging=timeaveraging,seldatavalue=datavalue,selname=FieldName
                  readfield,datadir,filetime,selgrid1,field,level,$
